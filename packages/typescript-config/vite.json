{
    "$schema": "https://json.schemastore.org/tsconfig",
    "extends": "./base.json",
    "Display": "Vite",
    "compilerOptions": {
        "allowJs": false,
        "esModuleInterop": false,
        "jsx": "react-jsx",
        "lib": ["ESNext", "DOM", "DOM.Iterable"],
        "noEmit": true,
        "resolveJsonModule": true,
        "target": "ESNext"
    }
}
