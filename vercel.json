{
    "version": 2,
    "builds": [
        {
            "src": "/apps/api/dist/main.js",
            "use": "@vercel/node"
        },
        {
            "src": "/apps/web/dist/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/apps/api/dist/main.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
        }
    ]
}
